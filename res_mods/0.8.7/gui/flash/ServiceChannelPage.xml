			<Gold>							<!-- Gold -->
				<Bold>true</Bold>							<!-- [true|false] Bold -->
				<FontSize>12</FontSize>						<!-- [n] Font size -->
				<ColorPlus>#66CC66</ColorPlus>				<!-- [#ffffff] Color of a positive result -->
				<ColorMinus>#F02828</ColorMinus>			<!-- [#ffffff] Color of a negative result -->
