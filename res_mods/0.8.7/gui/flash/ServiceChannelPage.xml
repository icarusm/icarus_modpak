<!-- Total per Battle + session Statistics  -->
<!-- Author: Vslav666, english translation by _Jeso_-->

<!-- To edit ServiceChannelPage.xml use an editor that supports UTF-8, for example Notepad or Notepad++. WordPad does not support this encoding and will "kill" the config file. -->

<!-- [] - Available options -->
<!-- true (true) - item is displayed -->
<!-- false (false) - the item is not displayed -->
<!-- n (number) - integer (fractions lead to crash) -->
<!-- s (string) - a string, all characters -->
<!-- c (char) - one character -->
<!-- # ffffff - color in a 16-hexadecimal calculation -->

<config>
	<Version>1.2</Version>						  			<!-- NOTE! Incorrect version of XML-config will hang the client -->
	
	<TypeMod>3</TypeMod>									<!-- [1|2|3|4] Fashion type of colored posts -->
															<!-- 1-standard, 2-MSM (Vslav, Set, V2, Stiv372, Veslo, V1), 3-YasenKrasen, 4-MCM (V0, V3, Hewlett) -->
	<PremiumAccount>true</PremiumAccount>					<!-- [true|false] Premium account -->
															<!-- true = Premium Account, false = Regular Account -->
	<SeparatorGroupsOfDigits>space</SeparatorGroupsOfDigits>	<!-- [c] thousands separator ("space" - for the Russian version, "." or "," - for the UK/US version -->
															<!-- c - a single character, if character "space", write the word "space" -->
	<TotalBattle>							<!-- TOTAL FOR BATTLE -->
		<Color>
			<Victory>default</Victory>						<!-- [green|yellow|red|grey|lightGrey|action|battleResult|default] Color for Victory -->
			<DrawGame>default</DrawGame>					<!-- [green|yellow|red|grey|lightGrey|action|battleResult|default] Color for Draw -->
			<Defeat>default</Defeat>						<!-- [green|yellow|red|grey|lightGrey|action|battleResult|default] Color for Defeat -->
															<!-- default - the default color is specified in the normal message files  -->
		</Color>
		<StartPos>0</StartPos>								<!-- [n] Indented from the left margin in pixels -->
		<Experience>						<!-- Experience per Battle -->
			<Visible>false</Visible>						<!-- [true|false] Visible -->
			<Title>Exp X1/X0/Free:</Title>					<!-- [s] Title text -->
			<Bold>false</Bold>								<!-- [true|false] Bold -->
			<FontSize>12</FontSize>							<!-- [n] Font size -->
			<Color>#CDC673</Color>							<!-- [#ffffff] Color in 16-hexadecimal -->
			<X1>							<!-- X1 experience per Battle -->
				<Visible>false</Visible>					<!-- [true| false] Visibility X1 experience per Battle -->
				<Bold>true</Bold>							<!-- [true|false] Bold -->
				<FontSize>12</FontSize>						<!-- [n] Font size -->			
				<Color>#1E90FF</Color>						<!-- [#ffffff] Color in 16-hexadecimal -->
				<Icon>2</Icon>								<!-- [0|1|2] Icon: 0-Hide, 1-Front, 2-After (works only for TypeMod 2 & 3) -->
			</X1>
			<X0>							<!-- X0 experience per Battle -->
				<Visible>false</Visible>					<!-- [true| false] Visibility X0 experience per Battle -->
				<Bold>true</Bold>							<!-- [true|false] Bold -->
				<FontSize>12</FontSize>						<!-- [n] Font size -->			
				<Color>#1E90FF</Color>						<!-- [#ffffff] Color in 16-hexadecimal -->
				<Icon>2</Icon>								<!-- [0|1|2] Icon: 0-Hide, 1-Front, 2-After (works only for TypeMod 2 & 3) -->
			</X0>
			<Free>							<!-- Free experience per Battle -->
				<Visible>false</Visible>					<!-- [true| false] Visibility free experience per Battle -->
				<Bold>true</Bold>							<!-- [true|false] Bold -->
				<FontSize>12</FontSize>						<!-- [n] Font size -->			
				<Color>#CD853F</Color>						<!-- [#ffffff] Color in 16-hexadecimal -->
				<Icon>2</Icon>								<!-- [0|1|2] Icon: 0-Hide, 1-Front, 2-After (works only for TypeMod 2 & 3) -->
			</Free>
		</Experience>
		<Repair>							<!-- Repair -->
			<Visible>true</Visible>							<!-- [true|false] Visible -->
			<Title>Repairs:</Title>							<!-- [s] Title text -->
			<Bold>false</Bold>								<!-- [true|false] Bold -->
