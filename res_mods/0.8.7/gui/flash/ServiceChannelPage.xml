	<Statistics>							<!-- Statistics -->
		<Color>grey</Color>									<!-- [green|yellow|red|grey|lightgrey|battleResult] Color background -->
		<Battle>							<!-- Battles -->
			<Visible>true</Visible>							<!-- [true|false] Visible -->
			<Title>Wins/Battles:</Title>					<!-- [s] Title text -->
			<Bold>false</Bold>								<!-- [true|false] Bold -->
			<FontSize>12</FontSize>							<!-- [n] Font size -->
			<Color>#929290</Color>							<!-- [#ffffff] Color in 16-hexadecimal -->
